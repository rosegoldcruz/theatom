{
  "name": "arbibot-pro-nextjs",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "compile": "npx hardhat compile",
    "test": "npx hardhat test",
    "test:coverage": "npx hardhat coverage",
    "deploy:local": "npx hardhat run scripts/deploy.cjs --network localhost",
    "deploy:goerli": "npx hardhat run scripts/deploy.cjs --network goerli",
    "deploy:sepolia": "npx hardhat run scripts/deploy.cjs --network sepolia",
    "deploy:base": "npx hardhat run scripts/deployBase.cjs --network base_sepolia",
    "deploy:base-mainnet": "npx hardhat run scripts/deployBase.cjs --network base_mainnet",
    "deploy:flashloan": "npx hardhat run scripts/deploy-flashloan-verified.js --network base_sepolia",
    "deploy:flashloan-mainnet": "npx hardhat run scripts/deploy-flashloan-verified.js --network base_mainnet",
    "quick:deploy": "npx hardhat run scripts/quick-deploy.js --network base_sepolia",
    "deploy:simple": "npx hardhat run scripts/deploy-simple-verified.js --network base_sepolia",
    "verify:contract": "npx hardhat verify --network base-sepolia",
    "deploy:mainnet": "npx hardhat run scripts/deploy.cjs --network mainnet",
    "node": "npx hardhat node",
    "clean": "npx hardhat clean",
    "bot:start": "node orchestrator/launcher.cjs",
    "bot:config": "node -e \"console.log(JSON.stringify(require('./orchestrator/config.cjs'), null, 2))\"",
    "advanced:start": "node launch-advanced.cjs",
    "advanced:config": "node -e \"console.log(JSON.stringify(require('./config/production.cjs'), null, 2))\"",
    "advanced:test": "npm run test && npm run advanced:config",
    "system:status": "node -e \"const MasterControl = require('./agents/MasterControl.cjs'); const config = require('./config/production.cjs'); const mc = new MasterControl(config); console.log('System ready for deployment')\"",
    "security:check": "node -e \"const config = require('./config/production.cjs'); console.log('Security configuration validated')\"",
    "agents:test": "node -e \"console.log('Testing agent imports...'); require('./agents/ScannerAgent.cjs'); require('./agents/RiskManagerAgent.cjs'); require('./agents/ExecutorAgent.cjs'); require('./agents/AICoordinatorAgent.cjs'); require('./agents/MasterControl.cjs'); console.log('✅ All agents loaded successfully')\"",
    "test:integration": "node scripts/integration-test.js",
    "launch:production": "node scripts/production-launch.js",
    "monitor:system": "node scripts/system-monitor.js",
    "health:check": "node -e \"const axios = require('axios'); axios.get('http://localhost:3000/api/health').then(r => console.log('✅ API Health:', r.data)).catch(e => console.error('❌ API Health:', e.message))\"",
    "db:migrate": "npx supabase db push",
    "db:reset": "npx supabase db reset",
    "contracts:verify": "npx hardhat verify --network base_sepolia",
    "orchestrator:build": "cd orchestrator && npm run build",
    "orchestrator:start": "cd orchestrator && npm start",
    "orchestrator:dev": "cd orchestrator && npm run dev"
  },
